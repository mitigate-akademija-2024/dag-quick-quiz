</div>
<div class="divider"></div>
<div class="overflow-x-auto">
  <table id="quizzes" class="table">
    <thead class="text-left">
      <tr>
        <th>Nosaukums</th>
        <th>Apraksts</th>
        <th>Izveidots</th>
        <th>Autors</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: 'quiz_row', collection: @quizzes, as: :quiz %>
    </tbody>
  </table>
</div>
